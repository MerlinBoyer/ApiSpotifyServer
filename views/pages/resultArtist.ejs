<% include header %>


<body style="background-color: #b27a33;">

	<h1> </h1>
	<a href="http://localhost:8080/index" class="ui button">New Search</a>

<% for(var i=0;i<data.length;i++) { %>
	<div class="ui container" style="padding-top: 20px;">
		<h1 class="ui horizontal header divider">
		  <i class="bar chart icon"></i>
		  <%= data[i].name %>
		</h1>

		<div class="ui inverted segment">
			<p>  Followers :  <%=  data[i].followers.total %></p>
			<p>  Popularity :  <%=  data[i].popularity %></p>
			<p>  Spotify ID :  <%=  data[i].id %></p>
			<p>  Link to listen :  <%=  data[i].external_urls.spotify %></p>
		</div>
	</div>
<% } %>



</body>


</html>
